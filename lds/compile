### FORWARD

TF_INC=$(python -c 'import tensorflow as tf; print(tf.sysconfig.get_include())')
TF_LIB=$(python -c 'import tensorflow as tf; print(tf.sysconfig.get_lib())')

#nvcc -std=c++11 -c amul_kernel_cube.cu.cc -o lib/amul_kernel_cube.cu.o -I $TF_INC -D GOOGLE_CUDA=1 -x cu -gencode arch=compute_61,code=sm_61  -Xcompiler -fPIC -O3 --expt-relaxed-constexpr -lprotobuf 

/usr/local/cuda/bin/nvcc -c amul_kernel_cube.cu.cc -o lib/amul_kernel_cube.cu.o -I $TF_INC -D GOOGLE_CUDA=1 -x cu -gencode arch=compute_61,code=sm_61  -Xcompiler -fPIC -O3 --expt-relaxed-constexpr -lprotobuf

g++ -shared -o lib/amul_kernel_cube.so amul_kernel_cube.cc lib/amul_kernel_cube.cu.o -I $TF_INC -fPIC -L$TF_LIB -ltensorflow_framework -D_GLIBCXX_USE_CXX11_ABI=0


#### GRAD

/usr/local/cuda/bin/nvcc -c amul_kernel_cube_grad.cu.cc -o lib/amul_kernel_cube_grad.cu.o -I $TF_INC -D GOOGLE_CUDA=1 -x cu -gencode arch=compute_61,code=sm_61 -Xcompiler -fPIC -O3 --expt-relaxed-constexpr

g++ -shared -o lib/amul_kernel_cube_grad.so amul_kernel_cube_grad.cc lib/amul_kernel_cube_grad.cu.o -I $TF_INC -fPIC -L$TF_LIB -ltensorflow_framework -D_GLIBCXX_USE_CXX11_ABI=0

